{
  "version": "1.0.0",
  "description": "Coding_school service for nodeys echo system with graphql implementation",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --watch --mode development",
    "start": "node build/app.production.js",
    "lint:r": "eslint src/ --ext .ts",
    "lint:r:fix": "npm run lint:r -- --fix",
    "serve": "node build/app.production.js",
    "prettify": "prettier --write src",
    "test": "jest --coverage --silent",
    "test:debug": "jest --coverage=false --watchAll --verbose",
    "test:watch": "jest --watchAll",
    "codegen": "graphql-codegen",
    "changelog": "auto-changelog -p -u && git add CHANGELOG.md",
    "changelog:debug": "auto-changelog -p --template json --output changelog-data.json",
    "prepare:husky": "husky install && npx husky add .husky/pre-commit \"yarn lint-staged\"",
    "prepare": "husky install",
    "migrate": "node build/seeds.development.js",
    "migrate:init": "migrate-mongo init",
    "migrate:create": "migrate-mongo create migrate_sample",
    "migrate:status": "migrate-mongo status",
    "migrate:up": "migrate-mongo up && migrate-mongo status",
    "migrate:down": "migrate-mongo down",
    "pkg:update": "ncu -u"
  },
  "auto-changelog": {
    "commitLimit": false,
    "unreleased": true,
    "output": "CHANGELOG.md",
    "template": "views/changelog-template.hbs",
    "issueUrl": "http://jira.user.com.br/browse/{id}",
    "replaceText": {
      "[Ff]eature:": "",
      "[Ff]ix:": "",
      "[Bb]reak:": "",
      "([A-Z]+-\\d+)": "[$1](http://jira.user.com.br/browse/$1) - "
    },
    "includeBranch": [
      "main"
    ]
  },
  "dependencies": {
    "@apollo/federation": "^0.37.1",
    "@graphql-tools/merge": "^8.3.10",
    "apollo-datasource": "^3.3.2",
    "apollo-server-core": "^3.11.1",
    "apollo-server-express": "^3.11.1",
    "apollo-server-plugin-base": "^3.7.1",
    "bluebird": "^3.7.2",
    "bull": "^4.10.1",
    "cors": "^2.8.5",
    "dataloader": "^2.1.0",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "graphql": "16",
    "graphql-scalars": "^1.20.0",
    "handlebars": "^4.7.7",
    "helmet": "^6.0.0",
    "ioredis": "^5.2.4",
    "joi": "^17.7.0",
    "lodash": "^4.17.21",
    "marked": "^4.2.2",
    "migrate-mongo": "^9.0.0",
    "mongo-seeding": "^3.7.2",
    "mongoose": "^6.7.1",
    "nodemailer": "^6.8.0",
    "odata-v4-mongodb": "^0.1.12",
    "prom-client": "^14.1.0",
    "reading-time": "^1.5.0",
    "reflect-metadata": "^0.1.13",
    "sanitize-html": "^2.7.3",
    "ulid": "^2.3.0",
    "winston": "^3.8.2",
    "winston-transport": "^4.5.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@graphql-codegen/cli": "^2.13.11",
    "@graphql-codegen/typescript": "^2.8.1",
    "@graphql-codegen/typescript-operations": "^2.5.6",
    "@graphql-codegen/typescript-resolvers": "^2.7.6",
    "@types/bull": "^3.15.9",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.14",
    "@types/faker": "^6.6.8",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.2.2",
    "@types/jest-in-case": "^1.0.6",
    "@types/lodash": "^4.14.188",
    "@types/marked": "^4.0.7",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^18.11.9",
    "@types/nodemailer": "^6.4.6",
    "@types/redis": "^4.0.10",
    "@types/sanitize-html": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "auto-changelog": "^2.4.0",
    "babel-jest": "^29.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.26.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-webpack-plugin": "^3.2.0",
    "faker": "^6.6.6",
    "husky": "^8.0.1",
    "jest": "^29.2.2",
    "jest-in-case": "^1.0.2",
    "jest-watch-typeahead": "^2.2.0",
    "lint-staged": "^13.0.3",
    "msw": "^0.47.4",
    "nodemon": "^2.0.20",
    "nodemon-webpack-plugin": "^4.8.1",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  },
  "homepage": "https://github.com/AhmedBHameed/coding_school/#readme",
  "bugs": {
    "url": "https://github.com/AhmedBHameed/coding_school/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit --skipLibCheck && lint-staged"
    }
  },
  "license": "ISC",
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "main": "app.js",
  "name": "auth_service",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AhmedBHameed/coding_school/.git"
  },
  "author": "Ahmed HAMEED"
}
